{
  "loggerType": "binary",
  "loggerModule": "heiDPI_logger",
  "loggerBinary": "../heidpi_logger_cpp_port2/build/heidpi_cpp",
  "loggerConfigPath": "config.yml",
  "outputFilePath": "flow_event.json",
  "scenarioPath": "scenarios.json",
  "strace": "enabled",

  "generatorParams": {
    "host": "127.0.0.1",
    "port": 7000,
    "rate": 2.0,
    "message_size": 128
  },
  "eventProbabilities": {
    "flow": 1,
    "daemon": 0,
    "error": 0,
    "packet": 0
  },
  "eventParams": {
    "--show-flow-events": "1",
    "--show-daemon-events": "0",
    "--show-packet-events": "0",
    "--show-error-events": "0"
  }
}
